# model.py
import torch
import torch.nn as nn
import torch.nn.functional as F

class IoTNet(nn.Module):
    def __init__(self):
        super(IoTNet, self).__init__()
        self.fc1 = nn.Linear(10, 32)
        self.fc2 = nn.Linear(32, 16)
        self.fc3 = nn.Linear(16, 2)

    def forward(self, x):
        x = F.relu(self.fc1(x))
        x = F.relu(self.fc2(x))
        return self.fc3(x)
